<?xml version="1.0" ?>
<Simulation verbosity="debug">
  <RunInfo>
    <WorkingDir>DataobjectsAttributes</WorkingDir>
    <Files>simple_flow_3eqn.i,
    simple_flow_3eqn_control.py</Files>
    <Sequence>TestDataObjectAttributes,TestDataObjectAttributesFromDataBase</Sequence>
    <batchSize>1</batchSize>
  </RunInfo>

  <Models>
    <Code name="MyRAVEN" subType="RAVEN">
      <executable>%FRAMEWORK_DIR%/../RAVEN-%METHOD%</executable>
    </Code>
  </Models>

  <Distributions>
    <Normal name="Gauss1">
      <mean>310</mean>
      <sigma>3</sigma>
      <lowerBound>300</lowerBound>
      <upperBound>320</upperBound>
    </Normal>
    <Normal name="ANormalDistribution">
      <mean>-4</mean>
      <sigma>2</sigma>
    </Normal>
  </Distributions>

  <Samplers>
    <Grid name="myGrid">
      <variable name="Components|pipe|Tw">
        <distribution>Gauss1</distribution>
        <grid construction="equal" steps="1" type="value">300 303</grid>
      </variable>
      <Distribution name="Dummy1">
        <distribution>ANormalDistribution</distribution>
        <grid construction="equal" steps="2" type="CDF">0.1 0.5</grid>
      </Distribution>
    </Grid>
  </Samplers>

  <Steps>
    <MultiRun name="TestDataObjectAttributes">
      <Input class="Files" type="">simple_flow_3eqn.i</Input>
      <Input class="Files" type="">simple_flow_3eqn_control.py</Input>
      <Model class="Models" type="Code">MyRAVEN</Model>
      <Sampler class="Samplers" type="Grid">myGrid</Sampler>
      <Output class="Databases" type="HDF5">testGridRavenDatabase</Output>
      <Output class="DataObjects" type="HistorySet">stories1</Output>
      <Output class="DataObjects" type="HistorySet">stories2</Output>
      <Output class="DataObjects" type="History">hst1</Output>
      <Output class="DataObjects" type="History">hst2</Output>
      <Output class="DataObjects" type="PointSet">outGridTPS1</Output>
      <Output class="DataObjects" type="PointSet">outGridTPS2</Output>
      <Output class="DataObjects" type="Point">outGridTP</Output>
      <Output class="OutStreamManager" type="Print">stories1_dump</Output>
      <Output class="OutStreamManager" type="Print">stories2_dump</Output>
      <Output class="OutStreamManager" type="Print">hst1_dump</Output>
      <Output class="OutStreamManager" type="Print">hst2_dump</Output>
      <Output class="OutStreamManager" type="Print">outGridTPS1_dump</Output>
      <Output class="OutStreamManager" type="Print">outGridTPS2_dump</Output>
      <Output class="OutStreamManager" type="Print">outGridTP_dump</Output>
    </MultiRun>
    <IOStep name="TestDataObjectAttributesFromDataBase">
      <Input class="Databases" type="HDF5">testGridRavenDatabase</Input>
      <Input class="Databases" type="HDF5">testGridRavenDatabase</Input>
      <Input class="Databases" type="HDF5">testGridRavenDatabase</Input>
      <Input class="Databases" type="HDF5">testGridRavenDatabase</Input>
      <Input class="Databases" type="HDF5">testGridRavenDatabase</Input>
      <Input class="Databases" type="HDF5">testGridRavenDatabase</Input>
      <Input class="Databases" type="HDF5">testGridRavenDatabase</Input>
      <Output class="DataObjects" type="HistorySet">stories1_from_DB</Output>
      <Output class="DataObjects" type="HistorySet">stories2_from_DB</Output>
      <Output class="DataObjects" type="History">hst1_from_DB</Output>
      <Output class="DataObjects" type="History">hst2_from_DB</Output>
      <Output class="DataObjects" type="PointSet">outGridTPS1_from_DB</Output>
      <Output class="DataObjects" type="PointSet">outGridTPS2_from_DB</Output>
      <Output class="DataObjects" type="Point">outGridTP_from_DB</Output>
      <Output class="OutStreamManager" type="Print">stories1_from_DB_dump</Output>
      <Output class="OutStreamManager" type="Print">stories2_from_DB_dump</Output>
      <Output class="OutStreamManager" type="Print">hst1_from_DB_dump</Output>
      <Output class="OutStreamManager" type="Print">hst2_from_DB_dump</Output>
      <Output class="OutStreamManager" type="Print">outGridTPS1_from_DB_dump</Output>
      <Output class="OutStreamManager" type="Print">outGridTPS2_from_DB_dump</Output>
      <Output class="OutStreamManager" type="Print">outGridTP_from_DB_dump</Output>
    </IOStep>
  </Steps>

  <Databases>
    <HDF5 name="testGridRavenDatabase"/>
  </Databases>

  <OutStreamManager>
    <Print name="stories1_from_DB_dump">
      <type>csv</type>
      <source>stories1_from_DB</source>
    </Print>
    <Print name="stories2_from_DB_dump">
      <type>csv</type>
      <source>stories2_from_DB</source>
    </Print>
    <Print name="hst1_from_DB_dump">
      <type>csv</type>
      <source>hst1_from_DB</source>
    </Print>
    <Print name="hst2_from_DB_dump">
      <type>csv</type>
      <source>hst2_from_DB</source>
    </Print>
    <Print name="outGridTPS1_from_DB_dump">
      <type>csv</type>
      <source>outGridTPS1_from_DB</source>
    </Print>
    <Print name="outGridTPS2_from_DB_dump">
      <type>csv</type>
      <source>outGridTPS2_from_DB</source>
    </Print>
    <Print name="outGridTP_from_DB_dump">
      <type>csv</type>
      <source>outGridTP_from_DB</source>
    </Print>
    <Print name="stories1_dump">
      <type>csv</type>
      <source>stories1</source>
    </Print>
    <Print name="stories2_dump">
      <type>csv</type>
      <source>stories2</source>
    </Print>
    <Print name="hst1_dump">
      <type>csv</type>
      <source>hst1</source>
    </Print>
    <Print name="hst2_dump">
      <type>csv</type>
      <source>hst2</source>
    </Print>
    <Print name="outGridTPS1_dump">
      <type>csv</type>
      <source>outGridTPS1</source>
    </Print>
    <Print name="outGridTPS2_dump">
      <type>csv</type>
      <source>outGridTPS2</source>
    </Print>
    <Print name="outGridTP_dump">
      <type>csv</type>
      <source>outGridTP</source>
    </Print>
  </OutStreamManager>

  <DataObjects>
    <Point name="outGridTP">
      <options>
        <inputRow>-1</inputRow>
        <operator>max</operator>
      </options>
      <Input>pipe_Area,pipe_Dh,Dummy1</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </Point>
    <Point historyName="2" name="outGridTP_from_DB">
      <options>
        <inputRow>-1</inputRow>
        <operator>max</operator>
      </options>
      <Input>pipe_Area,pipe_Dh,Dummy1</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </Point>
    <PointSet name="outGridTPS1">
      <options>
        <inputRow>1</inputRow>
        <outputRow>-1</outputRow>
      </options>
      <Input>pipe_Area,pipe_Dh,Dummy1</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </PointSet>
    <PointSet name="outGridTPS2">
      <options>
        <inputPivotValue>0.00011</inputPivotValue>
        <pivotParameter>time</pivotParameter>
        <outputPivotValue>0.00012345</outputPivotValue>
      </options>
      <Input>pipe_Area,pipe_Dh,Dummy1</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </PointSet>
    <PointSet name="outGridTPS1_from_DB">
      <options>
        <inputRow>1</inputRow>
        <outputRow>-1</outputRow>
      </options>
      <Input>pipe_Area,pipe_Dh,Dummy1</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </PointSet>
    <PointSet name="outGridTPS2_from_DB">
      <options>
        <inputPivotValue>0.00011</inputPivotValue>
        <pivotParameter>time</pivotParameter>
        <outputPivotValue>0.00012345</outputPivotValue>
      </options>
      <Input>pipe_Area,pipe_Dh,Dummy1</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </PointSet>
    <History name="hst1">
      <options>
        <inputRow>1</inputRow>
      </options>
      <Input>pipe_Area,pipe_Dh</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </History>
    <History name="hst2">
      <options>
        <inputRow>-1</inputRow>
        <pivotParameter>time</pivotParameter>
        <outputPivotValue>0.0002 0.0003 0.0004</outputPivotValue>
      </options>
      <Input>pipe_Area,pipe_Dh</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </History>
    <History historyName="1" name="hst1_from_DB">
      <options>
        <inputRow>1</inputRow>
      </options>
      <Input>pipe_Area,pipe_Dh</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </History>
    <History historyName="2" name="hst2_from_DB">
      <options>
        <inputRow>-1</inputRow>
        <pivotParameter>time</pivotParameter>
        <outputPivotValue>0.0002 0.0003 0.0004</outputPivotValue>
      </options>
      <Input>pipe_Area,pipe_Dh</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </History>
    <HistorySet name="stories1">
      <options>
        <inputRow>1</inputRow>
      </options>
      <Input>pipe_Area,pipe_Dh</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </HistorySet>
    <HistorySet name="stories2">
      <options>
        <inputRow>-1</inputRow>
        <pivotParameter>time</pivotParameter>
        <outputPivotValue>0.0002 0.0003 0.0004</outputPivotValue>
      </options>
      <Input>pipe_Area,pipe_Dh</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </HistorySet>
    <HistorySet name="stories1_from_DB">
      <options>
        <inputRow>1</inputRow>
      </options>
      <Input>pipe_Area,pipe_Dh</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </HistorySet>
    <HistorySet name="stories2_from_DB">
      <options>
        <inputRow>-1</inputRow>
        <pivotParameter>time</pivotParameter>
        <outputPivotValue>0.0002 0.0003 0.0004</outputPivotValue>
      </options>
      <Input>pipe_Area,pipe_Dh</Input>
      <Output>pipe_Hw,pipe_Tw,time</Output>
    </HistorySet>
  </DataObjects>

</Simulation>
