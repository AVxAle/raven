<?xml version="1.0" encoding="UTF-8"?>
<Simulation>
<RunInfo>
    <WorkingDir>SBO_DET</WorkingDir>
    <Files>TMI_test_PRA_transient_less_w_ss_out_restart_0831.rd,DET_histories_eqPb.csv,TMI_PRA_trans_DET_control.py,TMI.i,TMI_test_PRA_transient_less_w_ss_out_restart_0831_mesh.xda,TMI_test_PRA_transient_less_w_ss_out_restart_0831.xda</Files>
    <Sequence>DET,extract_hist_from_hdf5_eqPb</Sequence>
    <batchSize>8</batchSize>
</RunInfo>
<Models>
    <Code name='MyRAVEN' type='RAVEN'>%FRAMEWORK_DIR%/../RAVEN-%METHOD%</Code>
    <Filter name="csv_from_hdf5" type='PrintCSV'>all</Filter>
</Models>
<Distributions>
    <Normal name='crewSecPG'>
        <mean>1</mean>
        <sigma>0.001</sigma>
        <lowerBound>0</lowerBound>
        <upperBound>2</upperBound>
        <adjustement>up</adjustement>
    </Normal>
    <Normal name='crew1DG2CoupledDG1'>
        <mean>1</mean>
        <sigma>0.001</sigma>
        <lowerBound>0</lowerBound>
        <upperBound>2</upperBound>
        <adjustement>up</adjustement>
    </Normal>    
    <Normal name='crew1DG1'>
        <mean>1</mean>
        <sigma>0.4</sigma>
        <lowerBound>0</lowerBound>
        <upperBound>2</upperBound>
        <adjustement>scaling</adjustement>
    </Normal>
    <Triangular name='CladFailureDist'>
        <apex>1</apex>
        <min>-0.1</min>
        <max>3</max>
    </Triangular>
    <Triangular name='PrimPGrecovery'>
        <apex>1</apex>
        <min>-0.1</min>
        <max>3</max>
    </Triangular>
</Distributions>
<Samplers>
    <DynamicEventTree name='DET' print_end_xml='yes'>
      <BranchingSettings>
        <Variable type='Normal' distName='crew1DG1' BranchProbs='0.8 0.9'>crew1DG1</Variable>
        <Variable type='Normal' distName='crew1DG2CoupledDG1' BranchProbs='0.8 0.9'>crew1DG2CoupledDG1</Variable>
        <Variable type='Normal' distName='crewSecPG' BranchProbs='0.8 0.9'>crewSecPG</Variable>
        <Variable type='Triangular' distName='PrimPGrecovery' BranchProbs='0.9'>PrimPGrecovery</Variable>
        <Variable type='Triangular' distName='CladFailureDist' BranchProbs='0.05 0.1 0.15'>CladFailureDist</Variable>
      </BranchingSettings>
    </DynamicEventTree>     
</Samplers>
<Steps> 
    <MultiRun name='DET'>
        <Input   type='Files'      subtype=''                >TMI.i</Input>
        <Input   type='Files'      subtype=''                >TMI_PRA_trans_DET_control.py</Input>
        <Input   type='Files'      subtype=''                >TMI_test_PRA_transient_less_w_ss_out_restart_0831.xda</Input>
        <Input   type='Files'      subtype=''                >TMI_test_PRA_transient_less_w_ss_out_restart_0831_mesh.xda</Input>
        <Input   type='Files'      subtype=''                >TMI_test_PRA_transient_less_w_ss_out_restart_0831.rd</Input>        
        <Model   type='Models'     subtype='Code'            >MyRAVEN</Model>
        <Sampler type='Samplers'   subtype='DynamicEventTree'>DET</Sampler>
        <Output  type='DataBases' subtype='HDF5'              >DET_SBO_RUN_eqPb</Output>
    </MultiRun>  
    <SingleRun name='extract_hist_from_hdf5_eqPb'>
        <Input   type='DataBases' subtype='HDF5'   >DET_SBO_RUN_eqPb</Input>
        <Model   type='Models'   subtype='Filter' >csv_from_hdf5</Model>
        <Output  type='Files'    subtype=''       >DET_histories_eqPb.csv</Output>
    </SingleRun>       
</Steps>
<DataBases>
        <HDF5 name="DET_SBO_RUN_eqPb"/> 
</DataBases>
<Datas>
   <Histories name='stories_from_database'>
        <Input>DeltaTimeScramToAux,CladTempTreshold</Input>
        <Output>avg_temp_clad_CH2,CladDamaged</Output>
   </Histories>
</Datas>
</Simulation>



