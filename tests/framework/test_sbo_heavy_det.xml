<?xml version="1.0" encoding="UTF-8"?>
<Simulation>
<RunInfo>
    <WorkingDir>SBO_DET</WorkingDir>
    <Files>TMI_test_PRA_transient_less_w_ss_out_restart_0831.rd,DET_histories_eqPb.csv,TMI_PRA_trans_DET_control.py,TMI.i,TMI_test_PRA_transient_less_w_ss_out_restart_0831_mesh.xda,TMI_test_PRA_transient_less_w_ss_out_restart_0831.xda</Files>
    <Sequence>DET,extract_hist_from_hdf5_eqPb</Sequence>
    <batchSize>8</batchSize>
</RunInfo>
<Models>
    <Code name='MyRAVEN' subType='RAVEN'>%FRAMEWORK_DIR%/../RAVEN-%METHOD%</Code>
    <Filter name="csv_from_hdf5" subType='PrintCSV'>all</Filter>
</Models>
<Distributions>
    <Normal name='crewSecPG'>
        <mean>1</mean>
        <sigma>0.001</sigma>
        <lowerBound>0</lowerBound>
        <upperBound>2</upperBound>
        <adjustement>up</adjustement>
    </Normal>
    <Normal name='crew1DG2CoupledDG1'>
        <mean>1</mean>
        <sigma>0.001</sigma>
        <lowerBound>0</lowerBound>
        <upperBound>2</upperBound>
        <adjustement>up</adjustement>
    </Normal>    
    <Normal name='crew1DG1'>
        <mean>1</mean>
        <sigma>0.4</sigma>
        <lowerBound>0</lowerBound>
        <upperBound>2</upperBound>
        <adjustement>scaling</adjustement>
    </Normal>
    <Triangular name='CladFailureDist'>
        <apex>1</apex>
        <min>-0.1</min>
        <max>3</max>
    </Triangular>
    <Triangular name='PrimPGrecovery'>
        <apex>1</apex>
        <min>-0.1</min>
        <max>3</max>
    </Triangular>
</Distributions>
<Samplers>
    <DynamicEventTree name='DET' print_end_xml='yes' maxSimulationTime='2500.0'>
      <varibale name='crew1DG1' >
        <distribution type='Normal' ProbabilityThresholds='0.8 0.9'>crew1DG1</distribution>
      </varibale> 
      <varibale name='crew1DG2CoupledDG1' >
        <distribution type='Normal' ProbabilityThresholds='0.8 0.9'>crew1DG2CoupledDG1</distribution>
      </varibale> 
       <varibale name='crewSecPG' >
        <distribution type='Normal' ProbabilityThresholds='0.8 0.9'>crewSecPG</distribution>
      </varibale> 
      <varibale name='PrimPGrecovery' >
        <distribution type='Normal' ProbabilityThresholds='0.9'>PrimPGrecovery</distribution>
      </varibale>     
      <varibale name='CladFailureDist' >
        <distribution type='Triangular' ProbabilityThresholds='0.05 0.1 0.15'>CladFailureDist</distribution>
      </varibale>
    </DynamicEventTree>     
</Samplers>
<Steps> 
    <MultiRun name='DET'>
        <Input   class='Files'     type=''                >TMI.i</Input>
        <Input   class='Files'     type=''                >TMI_PRA_trans_DET_control.py</Input>
        <Input   class='Files'     type=''                >TMI_test_PRA_transient_less_w_ss_out_restart_0831.xda</Input>
        <Input   class='Files'     type=''                >TMI_test_PRA_transient_less_w_ss_out_restart_0831_mesh.xda</Input>
        <Input   class='Files'     type=''                >TMI_test_PRA_transient_less_w_ss_out_restart_0831.rd</Input>
        <Model   class='Models'    type='Code'            >MyRAVEN</Model>
        <Sampler class='Samplers'  type='DynamicEventTree'>DET</Sampler>
        <Output  class='DataBases' type='HDF5'              >DET_SBO_RUN_eqPb</Output>
    </MultiRun>  
    <SingleRun name='extract_hist_from_hdf5_eqPb'>
        <Input   class='DataBases' type='HDF5'   >DET_SBO_RUN_eqPb</Input>
        <Model   class='Models'    type='Filter' >csv_from_hdf5</Model>
        <Output  class='Files'     type=''       >DET_histories_eqPb.csv</Output>
    </SingleRun>       
</Steps>
<DataBases>
        <HDF5 name="DET_SBO_RUN_eqPb"/> 
</DataBases>
<Datas>
   <Histories name='stories_from_database'>
        <Input>DeltaTimeScramToAux,CladTempTreshold</Input>
        <Output>avg_temp_clad_CH2,CladDamaged</Output>
   </Histories>
</Datas>
</Simulation>



