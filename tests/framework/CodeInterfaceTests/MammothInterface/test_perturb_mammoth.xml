<?xml version="1.0" ?>
<Simulation verbosity="debug">
  <RunInfo>
    <WorkingDir>QuarterPellet_Example_2D</WorkingDir>
    <Sequence>MCTestMammoth</Sequence>
    <batchSize>1</batchSize>
  </RunInfo>

  <Files>
    <Input name="mammothInput" type="mammothInput|rattlesnakeInput">QuarterPin_withFuel.i</Input>
    <Input name="bisonInput" type="bisonInput">bison_2way_fuelcladelastic.i</Input>
    <Input name="bisonMesh" type="bison|mesh">QuarterPin_FuelClad_FineQUAD8.e</Input>
    <Input name="rattlesnakeMesh" type="rattlesnake|mesh">QuarterPin_FuelGapCladWater_QUAD8_Fine.e</Input>
    <Input name="M1" type="yakxsinput">M1_2D.xml</Input>
    <Input name="M2" type="yakxsinput">M2_2D.xml</Input>
    <Input name="M3" type="yakxsinput">M3_2D.xml</Input>
    <Input name="M4" type="yakxsinput">M4_2D.xml</Input>
    <Input name="M5" type="yakxsinput">M5_2D.xml</Input>
    <Input name="M6" type="yakxsinput">M6_2D.xml</Input>
    <Input name="M7" type="yakxsinput">M7_2D.xml</Input>
    <Input name="M8" type="yakxsinput">M8_2D.xml</Input>
    <Input name="M9" type="yakxsinput">M9_2D.xml</Input>
    <Input name="M10" type="yakxsinput">M10_2D.xml</Input>
    <Input name="M11" type="yakxsinput">M11_2D.xml</Input>
    <Input name="M12" type="yakxsinput">M12_2D.xml</Input>
    <Input name="M13" type="yakxsinput">M13_2D.xml</Input>
    <Input name="M14" type="yakxsinput">M14_2D.xml</Input>
    <Input name="M15" type="yakxsinput">M15_2D.xml</Input>
    <Input name="M16" type="yakxsinput">M16_2D.xml</Input>
    <Input name="M17" type="yakxsinput">M17_2D.xml</Input>
    <Input name="M18" type="yakxsinput">M18_2D.xml</Input>
    <Input name="M19" type="yakxsinput">M19_2D.xml</Input>
    <Input name="M20" type="yakxsinput">M20_2D.xml</Input>
    <Input name="M21" type="yakxsinput">M21_2D.xml</Input>
    <Input name="M22" type="yakxsinput">M22_2D.xml</Input>
    <Input name="M23" type="yakxsinput">M23_2D.xml</Input>
  </Files>

  <Models>
    <Code name="Mammoth" subType="MAMMOTH">
        <executable>%FRAMEWORK_DIR%/../../mammoth/mammoth-%METHOD%</executable>
    </Code>
  </Models>

  <Distributions>
    <Normal name="G1">
      <mean>1</mean>
      <sigma>0.05</sigma>
      <lowerBound>0.00001</lowerBound>
      <upperBound>10</upperBound>
    </Normal>
    <Normal name="G2">
      <mean>1</mean>
      <sigma>0.1</sigma>
      <lowerBound>0.0001</lowerBound>
      <upperBound>10</upperBound>
    </Normal>
  </Distributions>

  <Samplers>
    <MonteCarlo name="MC1">
      <samplerInit>
        <limit>10</limit>
      </samplerInit>
      <variable name="Rattlesnake|AuxVariables|Temp|initial_condition">
        <distribution>G1</distribution>
      </variable>
      <variable name="Bison|Materials|fuel_solid_mechanics_elastic|poissons_ratio">
        <distribution>G2</distribution>
      </variable>
    </MonteCarlo>
  </Samplers>

  <OutStreams>
    <Print name="out_mc_rattlesnake_dump">
      <type>csv</type>
      <source>outMC</source>
    </Print>
  </OutStreams>

  <DataObjects>
    <PointSet name="outMC">
      <Input>Rattlesnake|AuxVariables|Temp|initial_condition, Bison|Materials|fuel_solid_mechanics_elastic|poissons_ratio</Input>
      <Output>eigenvalue</Output>
    </PointSet>
  </DataObjects>

  <Steps>
    <MultiRun name="MCTestRattlesnake">
      <Input class="Files" type="">bison_2way_fuelcladelastic.i</Input>
      <Input class="Files" type="">QuarterPin_withFuel.i</Input>
      <Input class="Files" type="">QuarterPin_FuelGapCladWater_QUAD8_Fine.e</Input>
      <Input class="Files" type="">QuarterPin_FuelClad_FineQUAD8.e</Input>
      <Input class="Files" type="">M9_2D.xml</Input>
      <Input class="Files" type="">M8_2D.xml</Input>
      <Input class="Files" type="">M7_2D.xml</Input>
      <Input class="Files" type="">M6_2D.xml</Input>
      <Input class="Files" type="">M5_2D.xml</Input>
      <Input class="Files" type="">M4_2D.xml</Input>
      <Input class="Files" type="">M3_2D.xml</Input>
      <Input class="Files" type="">M2_2D.xml</Input>
      <Input class="Files" type="">M23_2D.xml</Input>
      <Input class="Files" type="">M22_2D.xml</Input>
      <Input class="Files" type="">M21_2D.xml</Input>
      <Input class="Files" type="">M20_2D.xml</Input>
      <Input class="Files" type="">M1_2D.xml</Input>
      <Input class="Files" type="">M19_2D.xml</Input>
      <Input class="Files" type="">M18_2D.xml</Input>
      <Input class="Files" type="">M17_2D.xml</Input>
      <Input class="Files" type="">M16_2D.xml</Input>
      <Input class="Files" type="">M15_2D.xml</Input>
      <Input class="Files" type="">M14_2D.xml</Input>
      <Input class="Files" type="">M13_2D.xml</Input>
      <Input class="Files" type="">M12_2D.xml</Input>
      <Input class="Files" type="">M11_2D.xml</Input>
      <Input class="Files" type="">M10_2D.xml</Input>
      <Model class="Models" type="Code">Mammoth</Model>
      <Sampler class="Samplers" type="MonteCarlo">MC1</Sampler>
      <Output class="DataObjects" type="PointSet">outMC</Output>
      <Output class="OutStreams" type="Print">out_mc_rattlesnake_dump</Output>
    </MultiRun>
  </Steps>

</Simulation>
