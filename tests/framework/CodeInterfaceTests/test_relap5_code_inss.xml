<?xml version="1.0" ?>
<Simulation verbosity="debug">
  <RunInfo>
    <WorkingDir>RELAP5interfaceTestModifiedVersionINSS</WorkingDir>
    <Sequence>INSS_RELAP5_step,io_test</Sequence>
    <batchSize>1</batchSize>
  </RunInfo>

  <Files>
    <Input name="h23_rosa5sb10C006-cb_tr.i" type="">h23_rosa5sb10C006-cb_tr.i</Input>
    <Input name="modelpar.inp" type="">modelpar.inp</Input>
    <Input name="tpfh2o" type="">tpfh2o</Input>
   <Input name="dummy_restart_file_with_custom_name.r" type="">dummy_restart_file_with_custom_name.r</Input>
  </Files>

  <Models>
    <Code name="INSS_JP_RELAP5test" subType="Relap5inssJp">
      <executable>PUT HERE THE PATH TO YOUR EXECUTABLE</executable>
      <!-- 
        The following clarg variable is used to add customized commands to add to the
        running command of the code. For example, it can be used to define a customized
        restart file name like in the following
        Note: If it is not provided, RELAP5 is going to look for the defaul file name for RESTART calculations
       -->
      <clargs type="text" arg="-r dummy_restart_file_with_custom_name.r"/>
    </Code>
  </Models>

  <Distributions>
    <Uniform name="break_size">
      <lowerBound>0.00004072</lowerBound>
      <upperBound>0.00008171</upperBound>
    </Uniform>
  </Distributions>

  <Samplers>
    <MonteCarlo name="mc_loca">
      <samplerInit>
        <limit>1</limit>
        <initialSeed>509</initialSeed>
      </samplerInit>
      <variable name="9100101:3">
        <distribution>break_size</distribution>
      </variable>
      <variable name="modelParTest">
          <distribution>break_size</distribution>
      </variable>
    </MonteCarlo>
  </Samplers>

  <Steps>
    <MultiRun name="INSS_RELAP5_step" re-seeding="1" pauseAtEnd="True">
      <Input class="Files" type="">h23_rosa5sb10C006-cb_tr.i</Input>
      <Input class="Files" type="">dummy_restart_file_with_custom_name.r</Input>
      <Input class="Files" type="">modelpar.inp</Input>
      <Input class="Files" type="">tpfh2o</Input>
      <Model class="Models" type="Code">INSS_JP_RELAP5test</Model>
      <Sampler class="Samplers" type="MonteCarlo">mc_loca</Sampler>
      <Output class="Databases" type="HDF5">INSS_RELAP5_database</Output>
      <Output class="DataObjects" type="HistorySet" >during_run_time_dep_pressure_function_of_loca_size</Output>
    </MultiRun>
    <IOStep name="io_test" pauseAtEnd="True">
      <Input class="Databases" type="HDF5">INSS_RELAP5_database</Input>
      <Output class="DataObjects" type="PointSet" >end_pressure_function_of_loca_size</Output>
      <Input class="Databases" type="HDF5">INSS_RELAP5_database</Input>
      <Output class="DataObjects" type="HistorySet" >time_dep_pressure_function_of_loca_size</Output>
      <Output class="OutStreams" type="Print" >pressure_function_of_loca_size_print</Output>
      <Output class="OutStreams" type="Plot" >max_pressure</Output>
      <Output class="OutStreams" type="Plot" >time_dep_pressure_function_of_loca_size_plot</Output>
    </IOStep>    
  </Steps>

  <DataObjects>
    <PointSet name="end_pressure_function_of_loca_size">
      <Input>9100101:3</Input>
      <Output>p_503150000,p_220010000</Output>
      <options>
        <operator>max</operator>
      </options>
      
    </PointSet>
    <HistorySet name="time_dep_pressure_function_of_loca_size">
      <Input>9100101:3</Input>
      <Output>p_503150000,p_220010000,time</Output>
    </HistorySet>
    <HistorySet name="during_run_time_dep_pressure_function_of_loca_size">
      <Input>9100101:3</Input>
      <Output>p_503150000,p_220010000,time</Output>
    </HistorySet>
  </DataObjects>

  <Databases>
    <HDF5 name="INSS_RELAP5_database" readMode="overwrite"/>
  </Databases>

  <OutStreams>
    <Print name="pressure_function_of_loca_size_print">
      <type>csv</type>
      <source>end_pressure_function_of_loca_size</source>
    </Print>
    <Plot dim="2" name="time_dep_pressure_function_of_loca_size_plot" overwrite="false">
      <plotSettings>
        <plot>
          <type>line</type>
          <x>time_dep_pressure_function_of_loca_size|Output|time</x>
          <y>time_dep_pressure_function_of_loca_size|Output|p_503150000</y>
        </plot>
      </plotSettings>
      <actions>
        <how>png</how>
      </actions>
    </Plot>
    <Plot dim="2" name="max_pressure" overwrite="false">
      <plotSettings>
        <plot>
          <type>scatter</type>
          <x>end_pressure_function_of_loca_size|Input|9100101:3</x>
          <y>end_pressure_function_of_loca_size|Output|p_503150000</y>
        </plot>
      </plotSettings>
      <actions>
        <how>png</how>
      </actions>
    </Plot>
  </OutStreams>

</Simulation>
