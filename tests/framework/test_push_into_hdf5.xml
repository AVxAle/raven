<?xml version="1.0" encoding="UTF-8"?>
<Simulation>
<RunInfo>
    <WorkingDir>test_extract_data_s_from_hdf5</WorkingDir>
<Files>TMI_test_PRA_transient_less_w_ss_out_restart_0831.rd-0,TMI_PRA_trans_MC_control.py,TMI.i,TMI_test_PRA_transient_less_w_ss_out_restart_0831_mesh.xda,TMI_test_PRA_transient_less_w_ss_out_restart_0831.xda</Files>
    <Sequence>MonteCarlo,test_extract,test_push_in,test_extract_again</Sequence>
    <batchSize>3</batchSize>
</RunInfo>
<Models>
    <Code name='MyRAVEN' subType='RAVEN'>%FRAMEWORK_DIR%/../RAVEN-%METHOD%</Code>
</Models>
<Samplers>
  <MonteCarlo     name='RAVENmc3'  limit='2' initial_seed='1'></MonteCarlo>     
</Samplers>
<Steps> 
    <MultiRun name='MonteCarlo'>
        <Input   class='Files'      type=''                >TMI.i</Input>
        <Input   class='Files'      type=''                >TMI_PRA_trans_MC_control.py</Input>
        <Input   class='Files'      type=''                >TMI_test_PRA_transient_less_w_ss_out_restart_0831.xda</Input>
        <Input   class='Files'      type=''                >TMI_test_PRA_transient_less_w_ss_out_restart_0831_mesh.xda</Input>
        <Input   class='Files'      type=''                >TMI_test_PRA_transient_less_w_ss_out_restart_0831.rd-0</Input>        
        <Model   class='Models'     type='Code'            >MyRAVEN</Model>
        <Sampler class='Samplers'   type='MonteCarlo'>RAVENmc3</Sampler>
        <Output  class='DataBases' type='HDF5'              >MC_TEST_EXTRACT_STEP</Output>
    </MultiRun>  
    <InOutFromDataBase name='test_extract'>
        <Input   class='DataBases' type='HDF5'   >MC_TEST_EXTRACT_STEP</Input>
        <Output  class='Datas'    type='Histories'>histories_from_database</Output>
        <Input   class='DataBases' type='HDF5'   >MC_TEST_EXTRACT_STEP</Input>
        <Output  class='Datas'    type='TimePoint'>timepoint_from_database</Output>
        <Input   class='DataBases' type='HDF5'   >MC_TEST_EXTRACT_STEP</Input>
        <Output  class='Datas'    type='History'>history_from_database</Output>
        <Input   class='DataBases' type='HDF5'   >MC_TEST_EXTRACT_STEP</Input>
        <Output  class='Datas'    type='TimePointSet'>timepointset_from_database</Output>
    </InOutFromDataBase>  
    <InOutFromDataBase name='test_push_in'>
        <Input  class='Datas'    type='Histories'>histories_from_database</Input>
        <Output   class='DataBases' type='HDF5'   >MC_TEST_PUSH_STEP</Output>
        <Input  class='Datas'    type='TimePoint'>timepoint_from_database</Input>
        <Output   class='DataBases' type='HDF5'   >MC_TEST_PUSH_STEP</Output>
        <Input  class='Datas'    type='History'>history_from_database</Input>
        <Output   class='DataBases' type='HDF5'   >MC_TEST_PUSH_STEP</Output>
        <Input  class='Datas'    type='TimePointSet'>timepointset_from_database</Input>
        <Output   class='DataBases' type='HDF5'   >MC_TEST_PUSH_STEP</Output>
    </InOutFromDataBase>  
    <InOutFromDataBase name='test_extract_again'>
        <Input   class='DataBases' type='HDF5'   >MC_TEST_PUSH_STEP</Input>
        <Output  class='Datas'    type='Histories'>histories_IN_database</Output>
        <Input   class='DataBases' type='HDF5'   >MC_TEST_PUSH_STEP</Input>
        <Output  class='Datas'    type='TimePoint'>timepoint_IN_database</Output>
        <Input   class='DataBases' type='HDF5'   >MC_TEST_PUSH_STEP</Input>
        <Output  class='Datas'    type='History'>history_IN_database</Output>
        <Input   class='DataBases' type='HDF5'   >MC_TEST_PUSH_STEP</Input>
        <Output  class='Datas'    type='TimePointSet'>timepointset_IN_database</Output>
    </InOutFromDataBase>  
</Steps>
<DataBases>
        <HDF5 name="MC_TEST_EXTRACT_STEP"/> 
        <HDF5 name="MC_TEST_PUSH_STEP"/> 
</DataBases>
<Datas>
   <Histories name='histories_from_database' printCSV='t'>
        <Input>DeltaTimeScramToAux,DG1recoveryTime</Input>
        <Output>avg_temp_clad_CH2,CladDamaged</Output>
   </Histories>
   <TimePoint name='timepoint_from_database' printCSV='t'>
        <Input name="1">DeltaTimeScramToAux,DG1recoveryTime</Input>
        <Output>avg_temp_clad_CH2,CladDamaged</Output>
   </TimePoint>
   <History name='history_from_database' printCSV='t'>
        <Input name="1">DeltaTimeScramToAux,DG1recoveryTime</Input>
        <Output>avg_temp_clad_CH2,CladDamaged</Output>
   </History>
   <TimePointSet name='timepointset_from_database' printCSV='t'>
        <Input>DeltaTimeScramToAux,DG1recoveryTime</Input>
        <Output>avg_temp_clad_CH2,CladDamaged</Output>
   </TimePointSet>
   <Histories name='histories_IN_database' printCSV='t'>
        <Input>DeltaTimeScramToAux,DG1recoveryTime</Input>
        <Output>avg_temp_clad_CH2,CladDamaged</Output>
   </Histories>
   <TimePoint name='timepoint_IN_database' printCSV='t'>
        <Input name="histories_from_database|0">DeltaTimeScramToAux,DG1recoveryTime</Input>
        <Output>avg_temp_clad_CH2,CladDamaged</Output>
   </TimePoint>
   <History name='history_IN_database' printCSV='t'>
        <Input name="histories_from_database|0">DeltaTimeScramToAux,DG1recoveryTime</Input>
        <Output>avg_temp_clad_CH2,CladDamaged</Output>
   </History>
   <TimePointSet name='timepointset_IN_database' printCSV='t'>
        <Input>DeltaTimeScramToAux,DG1recoveryTime</Input>
        <Output>avg_temp_clad_CH2,CladDamaged</Output>
   </TimePointSet>
</Datas>
</Simulation>



