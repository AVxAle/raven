<?xml version="1.0" encoding="UTF-8"?>
<Simulation verbosity='debug'>
    <RunInfo>
        <WorkingDir>PointSetFromCSV</WorkingDir>
        <Sequence>load,print</Sequence>
        <batchSize>1</batchSize>
    </RunInfo>

    <Files>
        <Input name='csv'>dump.csv</Input>
    </Files>

    <Steps>
        <IOStep name='load'>
            <Input  class='Files' type=''>csv</Input>
            <Output class='DataObjects' type='PointSet'>solns</Output>
        </IOStep>
        <IOStep name='print'>
            <Input  class='DataObjects'      type='PointSet'>solns</Input>
            <Output class='OutStreamManager' type='Print'   >dump2</Output>
        </IOStep>
    </Steps>

    <DataObjects>
        <PointSet name='solns'>
            <Input>x1,x2</Input>
            <Output>ans,ans2</Output>
        </PointSet>
    </DataObjects>

    <OutStreamManager>
        <Print name='dump2'><type>csv</type><source>solns</source></Print>
    </OutStreamManager>
</Simulation>
