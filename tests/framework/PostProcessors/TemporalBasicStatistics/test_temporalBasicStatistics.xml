<?xml version="1.0" ?>
<Simulation verbosity="debug">
    <RunInfo>
        <WorkingDir>BasicStatisticGeneral</WorkingDir>
                <Sequence>readIn,tpp</Sequence>
        <batchSize>100</batchSize>
    </RunInfo>
    <Files>
        <Input name="rawDataFile">./dataSet.csv</Input>
    </Files>
    <Models>
        <PostProcessor name='tbs' subType='DataMining'>
            <KDD lib='temporalBasicStatistics'>
                                <timeID>Time</timeID>
                <parameters>x1,x2</parameters>
                                <what>all</what>
            </KDD>
        </PostProcessor>

    </Models>

    <Steps>
        <IOStep name='readIn' pauseAtEnd = 'True'>
            <Input    class = 'Files'             type = ''              >rawDataFile</Input>
            <Output   class = 'DataObjects'       type = 'HistorySet'       >rawData</Output>
        </IOStep>

        <PostProcess name='tpp' pauseAtEnd = 'True'>
            <Input   class = 'DataObjects'       type = 'HistorySet'      >rawData</Input>
            <Model   class = 'Models'            type = 'PostProcessor' >tbs</Model>
                        <Output  class = 'DataObjects'           type = 'History'>ppData</Output>
                        <Output   class = 'OutStreams'  type = 'Print'         >dummy</Output>
                        <Output   class = 'OutStreams'  type = 'Plot'          >plotPPDataMean</Output>
                        <Output   class = 'OutStreams'  type = 'Plot'          >plotPPDataSigma</Output>
                        <Output   class = 'OutStreams'  type = 'Plot'          >plotPPDataSkewKurtosis</Output>
                        <Output   class = 'OutStreams'  type = 'Plot'          >plotPPDataCov</Output>
                        <Output   class = 'OutStreams'  type = 'Plot'          >plotPPDataCorr</Output>
                        <Output   class = 'OutStreams'  type = 'Plot'          >plotPPDataNSens</Output>
                        <Output   class = 'OutStreams'  type = 'Plot'          >plotPPDataVDSens</Output>
                        <Output   class = 'OutStreams'  type = 'Plot'          >plotPPDataSens</Output>
                </PostProcess>
    </Steps>

    <OutStreams>
                <Print name = 'dummy'>
            <type>csv</type>
            <source>ppData</source>
        </Print>
                <Plot dim="2" name="plotPPDataMean" overwrite="True" >
                        <plotSettings>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x1-expectedValue</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x1-median</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x1-percentile_5</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x1-percentile_95</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x2-expectedValue</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x2-median</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x2-percentile_5</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x2-percentile_95</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <xlabel>Time</xlabel>
                                <ylabel>x1,x2 (Mean, Median, 10%, 90%)</ylabel>
                        </plotSettings>
                        <actions>
                                <how>png</how>
                        </actions>
            </Plot>

                <Plot dim="2" name="plotPPDataSigma" overwrite="True" >
                        <plotSettings>
                                <gridSpace>2 1</gridSpace>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x1-sigma</y>
                                        <interpPointsX>300</interpPointsX>
                                        <gridLocation>
                                                <x>0</x>
                                                <y>0</y>
                                        </gridLocation>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x1-variance</y>
                                        <interpPointsX>300</interpPointsX>
                                        <gridLocation>
                                                <x>0</x>
                                                <y>0</y>
                                        </gridLocation>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x1-variationCoefficient</y>
                                        <interpPointsX>300</interpPointsX>
                                        <gridLocation>
                                                <x>0</x>
                                                <y>0</y>
                                        </gridLocation>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x2-sigma</y>
                                        <interpPointsX>300</interpPointsX>
                                        <gridLocation>
                                                <x>1</x>
                                                <y>0</y>
                                </gridLocation>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x2-variance</y>
                                        <interpPointsX>300</interpPointsX>
                                        <gridLocation>
                                                <x>1</x>
                                                <y>0</y>
                                        </gridLocation>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x2-variationCoefficient</y>
                                        <interpPointsX>300</interpPointsX>
                                        <gridLocation>
                                                <x>1</x>
                                                <y>0</y>
                                        </gridLocation>
                                </plot>
                        </plotSettings>
                        <actions>
                                <how>png</how>
                        </actions>
            </Plot>

                <Plot dim="2" name="plotPPDataSkewKurtosis" overwrite="True" >
                        <plotSettings>
                                <gridSpace>2 1</gridSpace>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x1-skewness</y>
                                        <interpPointsX>300</interpPointsX>
                                        <gridLocation>
                                                <x>0</x>
                                                <y>0</y>
                                        </gridLocation>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x1-kurtosis</y>
                                        <interpPointsX>300</interpPointsX>
                                        <gridLocation>
                                                <x>0</x>
                                                <y>0</y>
                                        </gridLocation>
                                        <xlabel>Time</xlabel>
                                        <ylabel>skewness-x1,kurtosis-x1</ylabel>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x2-skewness</y>
                                        <interpPointsX>300</interpPointsX>
                                        <gridLocation>
                                                <x>1</x>
                                                <y>0</y>
                                </gridLocation>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|x2-kurtosis</y>
                                        <interpPointsX>300</interpPointsX>
                                        <gridLocation>
                                                <x>1</x>
                                                <y>0</y>
                                        </gridLocation>
                                        <xlabel>Time</xlabel>
                                        <ylabel>skewness-x2,kurtosis-x2</ylabel>
                                </plot>
                        </plotSettings>
                        <actions>
                                <how>png</how>
                        </actions>
            </Plot>

                <Plot dim="2" name="plotPPDataCov" overwrite="True" >
                        <plotSettings>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|covariance-x1-x1</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|covariance-x1-x2</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|covariance-x2-x1</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|covariance-x2-x2</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <xlabel>Time</xlabel>
                                <ylabel>Covariance</ylabel>
                        </plotSettings>
                        <actions>
                                <how>png</how>
                        </actions>
            </Plot>

                <Plot dim="2" name="plotPPDataCorr" overwrite="True" >
                        <plotSettings>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|pearson-x1-x1</y>
                                        <interpPointsX>300</interpPointsX>
                                        <range>
                                          <ymin>-0.1</ymin><ymax>1.1</ymax>
                                        </range>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|pearson-x1-x2</y>
                                        <interpPointsX>300</interpPointsX>
                                        <range>
                                          <ymin>-0.1</ymin><ymax>1.1</ymax>
                                        </range>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|pearson-x2-x1</y>
                                        <interpPointsX>300</interpPointsX>
                                        <range>
                                          <ymin>-0.1</ymin><ymax>1.1</ymax>
                                        </range>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|pearson-x2-x2</y>
                                        <interpPointsX>300</interpPointsX>
                                        <range>
                                          <ymin>-0.1</ymin><ymax>1.1</ymax>
                                        </range>
                                </plot>
                                <xlabel>Time</xlabel>
                                <ylabel>Pearson</ylabel>
                        </plotSettings>
                        <actions>
                                <how>png</how>
                        </actions>
            </Plot>

                <Plot dim="2" name="plotPPDataNSens" overwrite="True" >
                        <plotSettings>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|NormalizedSensitivity-x1-x1</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|NormalizedSensitivity-x1-x2</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|NormalizedSensitivity-x2-x1</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|NormalizedSensitivity-x2-x2</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <xlabel>Time</xlabel>
                                <ylabel>NormalizedSensitivity</ylabel>
                        </plotSettings>
                        <actions>
                                <how>png</how>
                        </actions>
            </Plot>

                <Plot dim="2" name="plotPPDataVDSens" overwrite="True" >
                        <plotSettings>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|VarianceDependentSensitivity-x1-x1</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|VarianceDependentSensitivity-x1-x2</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|VarianceDependentSensitivity-x2-x1</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|VarianceDependentSensitivity-x2-x2</y>
                                        <interpPointsX>300</interpPointsX>
                                </plot>
                                <xlabel>Time</xlabel>
                                <ylabel>VarianceDependentSensitivity</ylabel>
                        </plotSettings>
                        <actions>
                                <how>png</how>
                        </actions>
            </Plot>

                <Plot dim="2" name="plotPPDataSens" overwrite="True" >
                        <plotSettings>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|sensitivity-x1-x1</y>
                                        <interpPointsX>300</interpPointsX>
                                        <range>
                                          <ymin>0</ymin><ymax>1.1</ymax>
                                        </range>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|sensitivity-x1-x2</y>
                                        <interpPointsX>300</interpPointsX>
                                        <range>
                                          <ymin>0</ymin><ymax>1.1</ymax>
                                        </range>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|sensitivity-x2-x1</y>
                                        <interpPointsX>300</interpPointsX>
                                        <range>
                                          <ymin>0</ymin><ymax>1.1</ymax>
                                        </range>
                                </plot>
                                <plot>
                                        <type>line</type>
                                        <x>ppData|Output|Time</x>
                                        <y>ppData|Output|sensitivity-x2-x2</y>
                                        <interpPointsX>300</interpPointsX>
                                        <range>
                                          <ymin>0</ymin><ymax>1.1</ymax>
                                        </range>
                                </plot>
                                <xlabel>Time</xlabel>
                                <ylabel>Sensitivity (x1,x2)</ylabel>
                        </plotSettings>
                        <actions>
                                <how>png</how>
                        </actions>
            </Plot>

    </OutStreams>
    <DataObjects>
      <History name='ppData'>
        <Input>n</Input>
        <Output>Time</Output>
      </History>
        <HistorySet name="rawData">
            <Input>n</Input>
            <Output>Time,x1,x2</Output>
        </HistorySet>
    </DataObjects>
</Simulation>
