<?xml version="1.0" encoding="UTF-8"?>
<Simulation verbosity="debug">


<RunInfo>
    <WorkingDir>historySampling</WorkingDir>
    <Sequence>FirstMRun,PP1,plot1,PP2,plot2,PP3,plot3,PP4,plot4,PP5,plot5</Sequence>
    <batchSize>1</batchSize>
</RunInfo>

<Files>
  <Input name="lorentzAttractor.py" type="">lorentzAttractor.py</Input>
</Files>

<Steps>
    <MultiRun name="FirstMRun" pauseAtEnd="True">
      <Input   class="DataObjects"      type="Point"         >inputPlaceHolder</Input>
      <Model   class="Models"           type="ExternalModel" >PythonModule</Model>
      <Sampler class="Samplers"         type="MonteCarlo"    >MC_external</Sampler>
      <Output  class="DataObjects"      type="PointSet"      >outMC</Output>
      <Output  class="Databases"        type="HDF5"          >out_db</Output>
      <Output  class="OutStreamManager" type="Print"         >PrintHistorySet_dump</Output>
    </MultiRun>
	  <IOStep name='plot1'>
	    <Input   class='DataObjects'      type='HistorySet'    >outMC_PP1</Input>
	    <Input   class='DataObjects'      type='HistorySet'    >outMC</Input>
	    <Output  class='OutStreamManager' type='Plot'          >plot1</Output>
	  </IOStep>
    <IOStep name='plot2'>
      <Input   class='DataObjects'      type='HistorySet'    >outMC_PP2</Input>
      <Input   class='DataObjects'      type='HistorySet'    >outMC</Input>
      <Output  class='OutStreamManager' type='Plot'          >plot2</Output>
    </IOStep>
    <IOStep name='plot3'>
      <Input   class='DataObjects'      type='HistorySet'    >outMC_PP3</Input>
      <Input   class='DataObjects'      type='HistorySet'    >outMC</Input>
      <Output  class='OutStreamManager' type='Plot'          >plot3</Output>
    </IOStep>
    <IOStep name='plot4'>
      <Input   class='DataObjects'      type='HistorySet'    >outMC_PP4</Input>
      <Input   class='DataObjects'      type='HistorySet'    >outMC</Input>
      <Output  class='OutStreamManager' type='Plot'          >plot4</Output>
    </IOStep>
    <IOStep name='plot5'>
      <Input   class='DataObjects'      type='HistorySet'    >outMC_PP5</Input>
      <Input   class='DataObjects'      type='HistorySet'    >outMC</Input>
      <Output  class='OutStreamManager' type='Plot'          >plot5</Output>
    </IOStep>
    <PostProcess name="PP1">
      <Input   class="DataObjects"      type="HistorySet"    >outMC</Input>
      <Model   class="Models"           type="PostProcessor" >historySamplingPP_uniform</Model>
      <Output  class="DataObjects"      type="HistorySet"    >outMC_PP1</Output>
      <Output  class="OutStreamManager" type="Print"         >PrintPPHistorySet_dump1</Output>
    </PostProcess>
    <PostProcess name="PP2">
      <Input   class="DataObjects"      type="HistorySet"    >outMC</Input>
      <Model   class="Models"           type="PostProcessor" >historySamplingPP_first</Model>
      <Output  class="DataObjects"      type="HistorySet"    >outMC_PP2</Output>
      <Output  class="OutStreamManager" type="Print"         >PrintPPHistorySet_dump2</Output>
    </PostProcess>
    <PostProcess name="PP3">
      <Input   class="DataObjects"      type="HistorySet"    >outMC</Input>
      <Model   class="Models"           type="PostProcessor" >historySamplingPP_second</Model>
      <Output  class="DataObjects"      type="HistorySet"    >outMC_PP3</Output>
      <Output  class="OutStreamManager" type="Print"         >PrintPPHistorySet_dump3</Output>
    </PostProcess>
    <PostProcess name="PP4">
      <Input   class="DataObjects"      type="HistorySet"    >outMC</Input>
      <Model   class="Models"           type="PostProcessor" >historySamplingPP_filt_first</Model>
      <Output  class="DataObjects"      type="HistorySet"    >outMC_PP4</Output>
      <Output  class="OutStreamManager" type="Print"         >PrintPPHistorySet_dump4</Output>
    </PostProcess>
    <PostProcess name="PP5">
      <Input   class="DataObjects"      type="HistorySet"    >outMC</Input>
      <Model   class="Models"           type="PostProcessor" >historySamplingPP_filt_second</Model>
      <Output  class="DataObjects"      type="HistorySet"    >outMC_PP5</Output>
      <Output  class="OutStreamManager" type="Print"         >PrintPPHistorySet_dump5</Output>
    </PostProcess>
</Steps>


<Models>
    <ExternalModel ModuleToLoad="lorentzAttractor" name="PythonModule" subType="">
      <variables>sigma,rho,beta,x,y,z,time,x0,y0,z0</variables>
    </ExternalModel>
    <PostProcessor name="historySamplingPP_uniform" subType="InterfacedPostProcessor">
       <method>HistorySetSampling</method>
       <samplingType>uniform</samplingType>
       <numberOfSamples>10</numberOfSamples>
       <timeID>time</timeID>
       <interpolation>cubic</interpolation>
    </PostProcessor>
    <PostProcessor name="historySamplingPP_first" subType="InterfacedPostProcessor">
       <method>HistorySetSampling</method>
       <samplingType>firstDerivative</samplingType>
       <numberOfSamples>10</numberOfSamples>
       <timeID>time</timeID>
       <interpolation>cubic</interpolation>
    </PostProcessor>
    <PostProcessor name="historySamplingPP_second" subType="InterfacedPostProcessor">
       <method>HistorySetSampling</method>
       <samplingType>secondDerivative</samplingType>
       <numberOfSamples>10</numberOfSamples>
       <timeID>time</timeID>
       <interpolation>cubic</interpolation>
    </PostProcessor>
    <PostProcessor name="historySamplingPP_filt_first" subType="InterfacedPostProcessor">
       <method>HistorySetSampling</method>
       <samplingType>filteredFirstDerivative</samplingType>
       <timeID>time</timeID>
       <tolerance>0.5</tolerance>
    </PostProcessor>
    <PostProcessor name="historySamplingPP_filt_second" subType="InterfacedPostProcessor">
       <method>HistorySetSampling</method>
       <samplingType>filteredSecondDerivative</samplingType>
       <timeID>time</timeID>
       <tolerance>0.1</tolerance>
    </PostProcessor>
</Models>


<Distributions>
  <Normal name="x0_distrib">
    <mean>4</mean>
    <sigma>1</sigma>
  </Normal>
  <Normal name="y0_distrib">
    <mean>4</mean>
    <sigma>1</sigma>
  </Normal>
  <Normal name="z0_distrib">
    <mean>4</mean>
    <sigma>1</sigma>
  </Normal>
</Distributions>


<Samplers>
  <MonteCarlo name="MC_external">
    <samplerInit>
      <limit>1</limit>
    </samplerInit>
    <variable name="x0">
      <distribution>x0_distrib</distribution>
    </variable>
    <variable name="y0">
      <distribution>y0_distrib</distribution>
    </variable>
    <variable name="z0">
      <distribution>z0_distrib</distribution>
    </variable>
  </MonteCarlo>
</Samplers>


<Databases>
  <HDF5 name="out_db"/>
</Databases>


<OutStreamManager>
  <Print name="PrintHistorySet_dump">
    <type>csv</type>
    <source>outMC</source>
  </Print>
  <Print name="PrintPPHistorySet_dump1">
    <type>csv</type>
    <source>outMC_PP1</source>
  </Print>
  <Print name="PrintPPHistorySet_dump2">
    <type>csv</type>
    <source>outMC_PP2</source>
  </Print>
  <Print name="PrintPPHistorySet_dump3">
    <type>csv</type>
    <source>outMC_PP3</source>
  </Print>
  <Print name="PrintPPHistorySet_dump4">
    <type>csv</type>
    <source>outMC_PP4</source>
  </Print>
  <Print name="PrintPPHistorySet_dump5">
    <type>csv</type>
    <source>outMC_PP5</source>
  </Print>
  <Plot name='plot1' dim='2' overwrite='False'>
    <plotSettings>
      <plot>
        <type>line</type>
        <interpolationType>cubic</interpolationType>
        <interpPointsX>100</interpPointsX>
        <x>outMC|Output|time</x>
        <y>outMC|Output|x</y>
      </plot>
      <plot>
        <type>scatter</type>
        <x>outMC_PP1|Output|time</x>
        <y>outMC_PP1|Output|x</y>
      </plot>
      <xlabel>time</xlabel>
      <ylabel>x</ylabel>
    </plotSettings>
    <actions>
      <how>pdf</how>
      <title>
        <text>Plot History</text>
      </title>
    </actions>
  </Plot>
  <Plot name='plot2' dim='2' overwrite='False'>
    <plotSettings>
      <plot>
        <type>line</type>
        <interpolationType>cubic</interpolationType>
        <interpPointsX>100</interpPointsX>
        <x>outMC|Output|time</x>
        <y>outMC|Output|x</y>
      </plot>
      <plot>
        <type>scatter</type>
        <x>outMC_PP2|Output|time</x>
        <y>outMC_PP2|Output|x</y>
      </plot>
      <xlabel>time</xlabel>
      <ylabel>x</ylabel>
    </plotSettings>
    <actions>
      <how>pdf</how>
      <title>
        <text>Plot History</text>
      </title>
    </actions>
  </Plot>
  <Plot name='plot3' dim='2' overwrite='False'>
    <plotSettings>
      <plot>
        <type>line</type>
        <interpolationType>cubic</interpolationType>
        <interpPointsX>100</interpPointsX>
        <x>outMC|Output|time</x>
        <y>outMC|Output|x</y>
      </plot>
      <plot>
        <type>scatter</type>
        <x>outMC_PP3|Output|time</x>
        <y>outMC_PP3|Output|x</y>
      </plot>
      <xlabel>time</xlabel>
      <ylabel>x</ylabel>
    </plotSettings>
    <actions>
      <how>pdf</how>
      <title>
        <text>Plot History</text>
      </title>
    </actions>
  </Plot>
  <Plot name='plot4' dim='2' overwrite='False'>
    <plotSettings>
      <plot>
        <type>line</type>
        <interpolationType>cubic</interpolationType>
        <interpPointsX>100</interpPointsX>
        <x>outMC|Output|time</x>
        <y>outMC|Output|x</y>
      </plot>
      <plot>
        <type>scatter</type>
        <x>outMC_PP4|Output|time</x>
        <y>outMC_PP4|Output|x</y>
      </plot>
      <xlabel>time</xlabel>
      <ylabel>x</ylabel>
    </plotSettings>
    <actions>
      <how>pdf</how>
      <title>
        <text>Plot History</text>
      </title>
    </actions>
  </Plot>
  <Plot name='plot5' dim='2' overwrite='False'>
    <plotSettings>
      <plot>
        <type>line</type>
        <interpolationType>cubic</interpolationType>
        <interpPointsX>100</interpPointsX>
        <x>outMC|Output|time</x>
        <y>outMC|Output|x</y>
      </plot>
      <plot>
        <type>scatter</type>
        <x>outMC_PP5|Output|time</x>
        <y>outMC_PP5|Output|x</y>
      </plot>
      <xlabel>time</xlabel>
      <ylabel>x</ylabel>
    </plotSettings>
    <actions>
      <how>pdf</how>
      <title>
        <text>Plot History</text>
      </title>
    </actions>
  </Plot>
</OutStreamManager>


<DataObjects>
    <Point name="inputPlaceHolder">
      <Input>x0,y0,z0</Input>
      <Output>OutputPlaceHolder</Output>
    </Point>
    <HistorySet name="outMC">
      <Input>x0,y0,z0</Input>
      <Output>time,x,y,z</Output>
    </HistorySet>
    <HistorySet name="outMC_PP1">
      <Input>x0,y0,z0</Input>
      <Output>time,x,y,z</Output>
    </HistorySet>
    <HistorySet name="outMC_PP2">
      <Input>x0,y0,z0</Input>
      <Output>time,x,y,z</Output>
    </HistorySet>
    <HistorySet name="outMC_PP3">
      <Input>x0,y0,z0</Input>
      <Output>time,x,y,z</Output>
    </HistorySet>
    <HistorySet name="outMC_PP4">
      <Input>x0,y0,z0</Input>
      <Output>time,x,y,z</Output>
    </HistorySet>
    <HistorySet name="outMC_PP5">
      <Input>x0,y0,z0</Input>
      <Output>time,x,y,z</Output>
    </HistorySet>
</DataObjects>


</Simulation>



