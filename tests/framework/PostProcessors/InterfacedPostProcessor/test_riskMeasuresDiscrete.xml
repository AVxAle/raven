<?xml version="1.0" ?>
<Simulation verbosity="silent">
  <RunInfo>
    <WorkingDir>RiskMeasuresDiscrete</WorkingDir>
    <Sequence>FirstMRun,PP</Sequence>
    <batchSize>100</batchSize>
    <internalParallel>True</internalParallel>
  </RunInfo>

  <Files>
    <Input name="lorentzAttractor.py" type="">lorentzAttractor.py</Input>
  </Files>
  
  <Distributions>
    <Exponential name='valveTime_distExp'>
      <lambda>0.00069444</lambda>
    </Exponential>
    <Exponential name='pump1Time_distExp'>
      <lambda>0.00138889</lambda>
    </Exponential>
    <Exponential name='pump2Time_distExp'>
      <lambda>0.00138889</lambda>
    </Exponential>
  </Distributions>

  <Samplers>
    <MonteCarlo name='MC'>
      <samplerInit>
        <limit>100</limit>
      </samplerInit>
      <variable name='valveTime'>
        <distribution>valveTime_distExp</distribution>
      </variable>
      <variable name='pump1Time'>
        <distribution>pump1Time_distExp</distribution>
      </variable>
      <variable name='pump2Time'>
        <distribution>pump2Time_distExp</distribution>
      </variable>
    </MonteCarlo>
  </Samplers>

  <Steps>
    <MultiRun name="FirstMRun" pauseAtEnd="True">
      <Input   class="DataObjects"  type="PointSet"        >inputPlaceHolder</Input>
      <Model   class="Models"       type="ExternalModel"   >PythonModuleReduced</Model>
      <Sampler class="Samplers"     type="MonteCarlo"      >MC</Sampler>
      <Output  class="DataObjects"  type="PointSet"        >outRun</Output>
    </MultiRun>
    <PostProcess name="PP">
      <Input   class="DataObjects"  type="PointSet"        >outRun</Input>
      <Model   class="Models"       type="PostProcessor"   >riskMeasuresDiscrete</Model>
      <Output  class="DataObjects"  type="PointSet"        >outPPS</Output>
      <Output  class="OutStreams"   type="Print"           >PrintPPS_dump</Output>
    </PostProcess>
  </Steps>

  <Models>
	  <ExternalModel name='PythonModule' subType='' ModuleToLoad='modelTH'>
	    <variables>pump1Time,pump2Time,valveTime,Tmax,outcome,pump1State,pump2State,valveState,failureTime</variables>
	  </ExternalModel>
    <ExternalModel name='PythonModuleReduced' subType='' ModuleToLoad='modelTHreduced'>
      <variables>pump1Time,pump2Time,valveTime,Tmax,outcome,pump1State,pump2State,valveState,failureTime</variables>
    </ExternalModel>
    
    <!--PostProcessor name="riskMeasuresDiscrete" subType="InterfacedPostProcessor">
      <method>RiskMeasuresDiscrete</method>
      <measures>B,FV,RAW,RRW</measures>
      <variables>pump1State</variables>
      <target>outcome</target>
    </PostProcessor>   
    <PostProcessor name="riskMeasuresDiscrete" subType="InterfacedPostProcessor">
      <Function class="Functions" type="External">varsConverter</Function>
      <method>RiskMeasuresDiscrete</method>
      <measures>B,FV,RAW,RRW</measures>
      <variable>pump1RelValue,pump2RelValue,valve1RelValue</variable>
      <target>outcome</target>
    </PostProcessor-->

    <PostProcessor name="riskMeasuresDiscrete" subType="InterfacedPostProcessor">
      <method>RiskMeasuresDiscrete</method>
      <measures>B,FV,RAW,RRW</measures>
      <variable R0values='0,240' R1values='1441,2880' extendData='False'>pump1Time</variable>
      <variable R0values='0,240' R1values='1441,2880' extendData='False'>pump2Time</variable>
      <variable R0values='0,240' R1values='1441,2880' extendData='False'>valveTime</variable>
      <target   values='1400,14000'>Tmax</target>
    </PostProcessor>
    
  </Models>

  <DataObjects>
    <PointSet name="inputPlaceHolder">
      <Input>pump1Time,pump2Time,valveTime</Input>
      <Output>OutputPlaceHolder</Output>
    </PointSet>
    <PointSet name="outRun">
      <Input>pump1Time,pump2Time,valveTime</Input>
      <Output>Tmax,outcome,failureTime</Output>
    </PointSet>
	  <PointSet name="outRunMod">
	    <Input>pump1Time,pump2Time,valveTime</Input>
	    <Output>outcome,pump1State,pump2State,valveState,failureTime</Output>
	  </PointSet>
    <PointSet name="outPPS">
      <Input> pump1Time_avg  , pump2Time_avg  , valveTime_avg</Input>
      <Output>pump1State_B   , pump2State_B   , valveState_B,
              pump1State_FV  , pump2State_FV  , valveState_FV,
              pump1State_RAW , pump2State_RAW , valveState_RAW,
              pump1State_RRW , pump2State_RRW , valveState_RRW</Output>
    </PointSet>
  </DataObjects>

  <OutStreams>
    <Print name="PrintPPS_dump">
      <type>csv</type>
      <source>outPPS</source>
    </Print>
  </OutStreams>
  
</Simulation>
