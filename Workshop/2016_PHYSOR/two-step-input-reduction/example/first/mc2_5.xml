<?xml version="1.0" ?> 
<MonteCarlo name="MCExternal"> 
  <samplerInit> 
    <limit>10000</limit>
    <initialSeed>42</initialSeed>
    <reseedEachIteration>True</reseedEachIteration>
  </samplerInit>
  <variable name="xlatent_0">
    <distribution dim="29">MultivariateNormalReduction</distribution>
  </variable>
  <variable name="xlatent_1">
    <distribution dim="13">MultivariateNormalReduction</distribution>
  </variable>
  <variable name="xlatent_2">
    <distribution dim="6">MultivariateNormalReduction</distribution>
  </variable>
  <variable name="xlatent_3">
    <distribution dim="30">MultivariateNormalReduction</distribution>
  </variable>
  <variable name="xlatent_4">
    <distribution dim="3">MultivariateNormalReduction</distribution>
  </variable>
  <variablesTransformation distribution="MultivariateNormalReduction">
    <latentVariables>xlatent_0,xlatent_1,xlatent_2,xlatent_3,xlatent_4</latentVariables>
    <manifestVariables>inp_vars</manifestVariables>
    <method>pca</method>
  </variablesTransformation>
</MonteCarlo>
