#  DAKOTA INPUT FILE 

strategy, 
        single
        tabular_graphics_data

method, 
        conmin_mfd 
        convergence_tolerance = 1.e-7
        #ncsu_direct
        max_function_evaluations = 100

variables,
        continuous_design = 2 
          lower_bounds = 9.0  9.0 
          upper_bounds = 11.0 11.0
        descriptor 'HeadPumpA' 'HeadPumpB'

interface,
	system
	  analysis_driver = 'run_raven_script'
	  parameters_file = 'params.in'
	  results_file    = 'results.out'
          file_save file_tag
	  
responses,
	num_objective_functions = 1
        numerical_gradients
        method_source dakota
        interval_type forward 
        fd_gradient_step_size = 1.e-4
	no_hessians
